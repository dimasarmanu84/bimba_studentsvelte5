#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules/why-is-node-running/node_modules:/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules:/Users/dimasarmanu/Public/project/dss/node_modules:/Users/dimasarmanu/Public/project/node_modules:/Users/dimasarmanu/Public/node_modules:/Users/dimasarmanu/node_modules:/Users/node_modules:/node_modules:/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules/why-is-node-running/node_modules:/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules:/Users/dimasarmanu/Public/project/dss/node_modules:/Users/dimasarmanu/Public/project/node_modules:/Users/dimasarmanu/Public/node_modules:/Users/dimasarmanu/node_modules:/Users/node_modules:/node_modules:/Users/dimasarmanu/Public/project/dss/dss_studentsvelte5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../why-is-node-running/cli.js" "$@"
else
  exec node  "$basedir/../why-is-node-running/cli.js" "$@"
fi
