{
  "name": "flowbite-svelte-admin-dashboard",
  "version": "1.0.6",
  "description": "Flowbite Admin Dashboard components for Svelte",
  "homepage": "https://flowbite-svelte-admin-dashboard.vercel.app/",
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "gen:exports": "svelte-lib-helpers exports",
    "gen:docspropvalue": "svelte-lib-helpers docspropvalue themesberg/flowbite-svelte-admin-dashboard",
    "gen:component-data-prop-value": "svelte-lib-helpers component-data-prop-value themesberg/flowbite-svelte-admin-dashboard",
    "copy:packagejson": "svelte-lib-helpers package",
    "lib-helpers": "pnpm gen:docspropvalue && gen:component-data-prop-value && pnpm format && pnpm package && pnpm gen:exports && pnpm copy:packagejson",
    "ch": "npx changeset",
    "cv": "npx changeset version",
    "cp": "npx changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/nodemailer": "^6.4.17",
    "apexcharts": "^4.7.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "1.11.0",
    "flowbite-svelte-icons": "^2.1.1",
    "globals": "^16.0.0",
    "highlight.js": "^11.11.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.12",
    "runatics": "^0.1.4",
    "runes-meta-tags": "^0.4.3",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "svelte-lib-helpers": "^0.4.26",
    "svelte-meta-tags": "^4.2.0",
    "svelte-printpdf": "^1.0.2-metadata",
    "svelte-rune-highlight": "^0.6.6",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "peerDependencies": {
    "flowbite-svelte": "^1.0.0",
    "flowbite-svelte-icons": "^2.0.0",
    "svelte": "^5.0.0"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "tailwindcss",
    "flowbite",
    "ui",
    "admin dashboard",
    "signup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themesberg/flowbite-svelte-admin-dashboard.git"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@auth/sveltekit": "^1.10.0",
    "@castlenine/svelte-qrcode": "^2.3.0",
    "@google/gemini-cli": "^0.1.9",
    "@sveltejs/adapter-node": "^5.2.13",
    "@tinymce/tinymce-svelte": "^3.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/qrcode": "^1.5.5",
    "@vincjo/datatables": "^2.5.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.5",
    "nodemailer": "^6.10.1",
    "otp-io": "^1.2.7",
    "qrcode": "^1.5.4",
    "simple-svelte-autocomplete": "^2.5.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  }
}
